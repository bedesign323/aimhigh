}

.region-nav-main,
.search-toggle{
	@include inline-block;

	@include bp(m){
		display: block;
	}
}

.search-toggle{
	width: 30px;
	height: 30px;
	margin-left: 60px;
	margin-right: -100px;
	position: relative;
	top: -5px;
	cursor: pointer;
	
	.icon-search{
		@include transition;
		color: $color_accent;
		font-size: 30px;

		&:hover{
			color: $color_main;
		}
	}

	@include bp(m){
		top: auto;
		margin: 20px 0;
		display: block;
		width: auto;
		height: auto;
		text-align: center;
	}
	
	@include bp(m){
		display: none;
	}
	
}

#main-nav{
	@include transition;
	@extend %text_btn;

	transition: top 0;
	
	text-align: center;
	position: fixed;
	z-index: 100;
	top: 60px;
	left: 0;
	width: 100%;
	padding-top: 10px;
	background-color: #fff;

	.block{
		padding: 0 !important;
	}

	ul li ul{
		position: absolute;
		width: 150px;
		left: -999em;
		top: 35px;
		margin-left: -75px;
		
		opacity: 0;
		

		li{
			float: none;
			display: block;
			padding-bottom: 0;

			a{
				color: #fff;
				padding: 8px 0;
				display: block;
				background-color: $color_accent;

				&:hover{
					color: $color_main;
				};
			}

			

			&.first a{
				padding-top: 15px;
			};

			&.last a{
				padding-bottom: 15px;
			};

			&:before,
			&.last:before{
				content: none;
			};
		}

		&:after{
			bottom: 100%;
			left: 50%;
			border: solid transparent;
			content: " ";
			height: 0;
			width: 0;
			position: absolute;
			pointer-events: none;
			border-color: rgba(228, 186, 39, 0);
			border-bottom-color: #e4ba27;
			border-width: 6px;
			margin-left: -6px;
		};
	}

	li{
		@extend .inline_block;
		position: relative;
		padding-bottom: 25px;

		&:hover ul{
			left: 50%;
			opacity: 1;
		};
	}

	li + li:before{
		@extend %bull;
		content: 'â€¢';
		
	}

	li.fancy-sep-parent:before{
		content: '~';
		font-size: 30px;
		font-weight: 100;
		position: relative;
		top: 4px;
	}

	a{
		@extend %a_color_lt;

		padding-bottom: 5px;

		&.active,
		&.active-trail{
			
			border-bottom: $color_accent solid 1px;
		}
	}


	
	@include bp(m){
		position: relative;
		top: auto;
		left: auto;
		background-color: transparent;

		ul{
			padding: 30px 0;
		}

		li{
			display: block;
			padding: 10px 0;
			
			&:before{
				display: none !important;

			}

			a{
				color: #fff;

				&:hover{
					color: $color_main;
				}
			}
		}
	}

}

#main{
	@include transition;
	padding-left: 0;
	position: relative;
	z-index: 1;
	width: 100%;

	.inner{
		padding: 180px 0 0;

		@include bp(m){
