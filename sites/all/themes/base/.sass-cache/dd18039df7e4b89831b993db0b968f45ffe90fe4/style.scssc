/* TEASER */
/* default */
.article.teaser{
	@extend .clearfix;
	padding-bottom: 30px;
	margin: 0 auto 90px;
	
	
	.header{
		position: relative;
		margin-bottom: 25px;

		@include bp(s){
			margin-bottom: 0;
		}
	}

	.cover{
		line-height: 0;
		background-color: $color_accent;

		img{
			max-width: 100%;
			height: auto;
			@include transition;

			&:hover{
				opacity: .8;
			}
		}
	}

	.title{
		@extend .inline_block;
		position: absolute;
		left: 0;
		bottom: 30px;
		font-size: 34px;
		background-color: #fff;
		padding: 10px 20px 15px;
		margin-left: -50px;
		margin-right: 30px;

		@include bp(m){
			margin-left: 30px;
		}

		@include bp(s){
			margin-left: 10px;
			margin-right: 10px;
			bottom: -10px;
			padding: 10px 5px 0;
			display: block;
			position: relative;
			margin-top: -30px;
			
			h2{
				font-size: 18px;
				font-weight: 200;
				text-align: center;
			}
		}
	}
	
	.meta-data{
		float: left;
		width: 30%;
		text-align: center;
		margin-right: 6%;
		padding-top: 5px;

		.post-date{
			border-bottom: $border_solid_lt;
			padding-bottom: 15px;
			margin-bottom: 10px;
		}

		.author{
			margin-bottom: 10px;
		}

		@include bp(s){
			display: none;
			float: none;
			margin: 0;
			width: 100%;
			padding: 0 0 10px;
			margin-bottom: 20px;
			border-bottom: $border_solid_lt;

			.post-date,
			.comment-count,
			.author{
				@include inline-block;
				border: none;
				margin: 0 5px;
				padding: 0;
				letter-spacing: 0 !important;
			}
		}
	}
	

	.summary{
		float: left;
		width: 64%;
		text-align: justify;

		a{
			@extend %text_btn;
			margin-left: 5px;
		}

		@include bp(s){
			width: 100%;
			display: none;
		}
	}

	.footer{
		border-top: $border_dashed_lt;
		clear: both;
		margin-top: 30px;
		padding-top: 5px;

		@include bp(s){
			margin-top: 20px;
			display: none;
		}
	}

	.sections{
		float: right;
	
		a{
			@extend .inline_block;
			@extend %text_btn;
			margin-left: 20px;
		}
	}

	@include bp(s){
		margin-bottom: 40px;
		padding-bottom: 40px;
		border-bottom: $border_dashed_accent;
	}
}

